{
   "connectorTypes": {
       "Http": "./connectors/http-connector",
       "VacuumPump": "./connectors/vacuum-pump-connector"
   },

   "cloudConnectors": {
       "wc-http": {
           "type": "Http",
           "config": {
               "pollFrequency": 10000,
               "url": "https://api-iot.analoggarage.com",
               "headers": {
                   "apiKey": "witchcraft",
                   "content-type": "application/json"
               },
               "mac": "rasp-test-01"
           }
       }
   },

   "deviceConnectors": {
       "wc-vp-1": {
           "type": "VacuumPump",
           "config": {
               "pollFrequency": 5000,
               "portName": "/dev/tty.usbserial-FTGPJQKL",
               "baudRate": 9600,
               "parity": "none",
               "stopBits": 1,
               "dataBits": 8,
               "flowControl": false
           }
       }
   }
}
