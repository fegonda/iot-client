{
    "connectorTypes": {
        "Http": "./http-connector",
        "EbaraPump": "./ebara-pump-connector",
        "Mqtt": "./mqtt-connector",
        "ButtonMqtt": "./button-mqtt-connector",
        "CncCloud": "./cnc-cloud-connector",
        "CncGateway": "./cnc-gateway-connector",
        "LeptonCamera": "./lepton-camera-connector"
    },

    "cloudConnectors": {
        "testgw-cnc-cloud": {
            "type": "CncCloud",
            "config": {
                "host": "api-iot-dev.analoggarage.com",
                "port": 8443,
                "protocol": "mqtts",
                "networkInterface": "en0",
                "gatewayname": "testgw",
                "username": "",
                "password": "",
                "topics": ""
            }
        }
    },

    "deviceConnectors": {
        "testgw-cnc-gateway": {
            "type": "CncGateway",
            "config": {
            }
        }
    },

    "cloudConnectors.ignore": {
        "http": {
            "type": "Http",
            "config": {
                "pollFrequency": 10000,
                "url": "https://api-iot.analoggarage.com",
                "headers": {
                    "authorization": "gateway1|__hash__key__",
                    "content-type": "application/json"
                }
            }
        }
    },

    "deviceConnectors.ignore": {
        "lepton-camera0": {
            "type": "LeptonCamera",
            "config": {
                "pollFrequency": 5000,
                "spiDevice": "/dev/spidev0.1",
                "i2cDevice": "/dev/i2c-1",
                "maxRetries": 600,
                "cameraResetPin": 23
            }
        },

        "ebara-pump0": {
            "type": "EbaraPump",
            "config": {
                "pollFrequency": 5000,
                "portName": "/dev/pump0",
                "baudRate": 9600,
                "parity": "none",
                "stopBits": 1,
                "dataBits": 7,
                "flowControl": false,
                "pumpRequestTimeout": 60000
            }
        },

        "buttons-hub0": {
            "type": "ButtonMqtt",
            "config": {
                "host": "10.0.0.1",
                "port": 1883,
                "networkInterface": "en0",
                "username": "esp",
                "password": "buddon",
                "topics": "/things/+/+"
            }
        }
    }
}
